d2js.modify = function(row, columns){
	$V(this, row, {
		name : V.notNull,
		email : [V.email, V.shortest(10)],
		info : V.attrs(['linkin']),
		'info,linkin': V.shortest(5)
	});
	
	return this.updateRow('author', row, columns);
}


